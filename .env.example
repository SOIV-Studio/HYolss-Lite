# Main Bot Token (Production)
DISCORD_TOKEN=your_main_token_here
DISCORD_CLIENT_ID=your_main_client_id_here
DISCORD_GUILD_ID=your_main_guild_id_here

# Development Bot Token
DEV_DISCORD_TOKEN=your_dev_token_here
DEV_DISCORD_CLIENT_ID=your_dev_client_id_here
DEV_DISCORD_GUILD_ID=your_dev_guild_id_here

# Environment Flag (set to 'development' or 'production')
NODE_ENV=production

# PostgreSQL Database Configuration (Legacy)
DB_USER=your_user
DB_HOST=localhost
DB_NAME=your_NAME
DB_PASSWORD=your_password
DB_PORT=5432

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_api_key_here
SUPABASE_SECRET=your_supabase_service_role_key_here

# MongoDB Atlas Configuration
MONGODB_URI=your_mongodb_connection_string_here
MONGODB_DB_NAME=your_NAME_here

# Auto-Update Configuration
ADMIN_WEBHOOK_URL=your_discord_webhook_url_for_admin_server_here
BOT_DEVELOPER_IDS=your_discord_id,another_developer_id